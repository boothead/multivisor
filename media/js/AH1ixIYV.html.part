	this.CssClass = 'dp-j';
	this.Style =	'.dp-j .annotation { color: #646464; }' +
					'.dp-j .number { color: #C00000; }';
}

dp.sh.Brushes.Processing.prototype	= new dp.sh.Highlighter();
dp.sh.Brushes.Processing.Aliases	= ['processing'];</script>  
